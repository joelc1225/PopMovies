<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config>
        <domain includeSubdomains="true">themoviedb.org</domain>
        <base-config cleartextTrafficPermitted="true">

            <trust-anchors>
                <certificates src="user" />
                <certificates src="system" />
            </trust-anchors>
        </base-config>
    </domain-config>
</network-security-config>